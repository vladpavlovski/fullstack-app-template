{
  "name": "fullstack-app-template",
  "version": "0.0.1",
  "private": true,
  "author": "Vlad Pavlovski",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "studio": "npx prisma studio",
    "format": "prettier --write .",
    "lint": "next lint",
