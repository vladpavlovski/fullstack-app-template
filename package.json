    "eslint-config-next": "12.2.5",
    "eslint-config-prettier": "8.5.0",
    "postcss": "8.4.16",
    "prettier": "2.7.1",
    "prettier-plugin-prisma": "4.2.0",
    "prettier-plugin-tailwindcss": "0.1.13",
    "prisma": "4.3.1",
    "tailwindcss": "3.1.8",
    "ts-node": "10.9.1",
    "typescript": "4.8.2"
  }
