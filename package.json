    "@prisma/client": "4.3.1",
    "apollo-server-micro": "3.10.2",
    "aws-sdk": "2.1209.0",
    "graphql": "16.6.0",
    "micro": "9.4.1",
    "micro-cors": "0.1.1",
    "next": "12.2.5",
    "nexus": "1.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.34.2",
    "react-hot-toast": "2.3.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "12.2.5",
    "@tailwindcss/forms": "0.5.3",
    "@tailwindcss/typography": "0.5.7",
    "@types/eslint": "8.4.6",
    "@types/node": "18.7.15",
    "@types/react": "18.0.18",
